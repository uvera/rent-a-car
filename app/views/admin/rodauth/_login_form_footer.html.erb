<% unless rodauth(:admin).login_form_footer_links.empty? %>
  <ul>
    <% rodauth(:admin).login_form_footer_links.sort.each do |_, link, text| %>
      <li><%= link_to text, link, class: "text-orange-600 rounded-sm text-sm hover:underline hover:text-orange-800 focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-orange-600 dark:text-orange-300 dark:hover:text-orange-400 dark:focus-visible:ring-2 dark:focus-visible:ring-orange-300" %></li>
    <% end %>
  </ul>
<% end %>
